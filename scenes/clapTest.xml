<SCENE>

<INITIALS>
    <frustum near="0.1" far="100"/>                 
    <translation x="-1" y="1" z="0" />               
    <rotation axis="x" angle="0" />                
    <rotation axis="y" angle="0" />                
    <rotation axis="z" angle="0" />                
    <scale sx="0.25" sy="0.25" sz="0.25" />         
    <reference length="5" />                        
</INITIALS>


<ILLUMINATION>
    <ambient r="0.2" g="0.2" b="0.2" a="1" />       
    <background r="0" g="0" b="0" a="1" />          
</ILLUMINATION>


<LIGHTS>	
	<LIGHT id="light0">                                    
        <enable value ="1" />                   	       
        <position  x="0" y="10" z="0" w="1" />       	   
        <ambient r="0.2" g="0.2" b="0.2" a="1.0" />        	 
        <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />        
        <specular r="1.0" g="1.0" b="1.0" a="1.0" />      
    </LIGHT>
	
	<LIGHT id="light1">                                    
        <enable value ="1" />                   	      
        <position x="30" y="10" z="0" w="1" />       	   
        <ambient r="0.2" g="0.2" b="0.2" a="1.0" />       
        <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />        
        <specular r="1.0" g="1.0" b="1.0" a="1.0" />       
    </LIGHT>
	
	<LIGHT id="light2">                                   
        <enable value ="1" />                   	       
        <position x="0" y="10" z="20" w="1" />       		   
        <ambient r="0.2" g="0.2" b="0.2" a="1.0" />      
        <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />        
        <specular r="1.0" g="1.0" b="1.0" a="1.0" />       
    </LIGHT>

    <LIGHT id="light3">                                   
        <enable value ="1" />                   	       
        <position x="30" y="10" z="20" w="1" />       			
        <ambient r="0.2" g="0.2" b="0.2" a="1.0" />           	
        <diffuse r="0.6" g="0.6" b="0.6" a="1.0" />       
        <specular r="0.6" g="0.6" b="0.6" a="1.0" />      
    </LIGHT>
    
</LIGHTS>

<TEXTURES>
    <TEXTURE id="balconyWall">
        <file path="images/balconyWall.png" />               
        <amplif_factor s="1" t="1" />                           
    </TEXTURE>

    <TEXTURE id="stairsWall">
        <file path="images/stairsWall.png" />      
        <amplif_factor s="1" t="1" />                           

    </TEXTURE>

     <TEXTURE id="doorsWall">
        <file path="images/doorsWall.png" />                
        <amplif_factor s="1" t="1" />                           
                            
    </TEXTURE>

	<TEXTURE id="floor">
        <file path="images/floor.png" />               
        <amplif_factor s="1" t="1" />                                                    
    </TEXTURE>

    <TEXTURE id="counter">
        <file path="images/counter.png" />      
        <amplif_factor s="1" t="1" />                           

    </TEXTURE>

    <TEXTURE id="counterSide">
        <file path="images/counterSide.png" />               
        <amplif_factor s="1" t="1" />                           
    </TEXTURE>
    
    <TEXTURE id="heart">
        <file path="images/heart.png" />               
        <amplif_factor s="1" t="1" />                           
    </TEXTURE>

    <TEXTURE id="cornerCube">
        <file path="images/cornerCube.png" />               
        <amplif_factor s="1" t="1" />                           
    </TEXTURE>
    
    <TEXTURE id="sideCube">
        <file path="images/sideCube.png" />               
        <amplif_factor s="1" t="1" />                           
    </TEXTURE>
    
</TEXTURES>


<MATERIALS>
    <MATERIAL id="piramideMaterial">                                  
        <shininess value="1" />
        <specular r="0" g="0" b="0" a="1" />        
        <diffuse r="1.0" g="0.68" b="0.4" a="1" />        
        <ambient r="0.2" g="0.2" b="0.2" a="1" />         
        <emission r="0.0" g="0.0" b="0.0" a="1" />        
    </MATERIAL>
	
	<MATERIAL id="leavesTreeMaterial">                                  
        <shininess value="1" />
        <specular r="0.2" g="0.2" b="0.2" a="1" />        
        <diffuse r="0.0" g="0.207" b="0.0" a="1" />       
        <ambient r="0.1" g="0.1" b="0.1" a="1" />         
        <emission r="0.0" g="0.0" b="0.0" a="1" />        
    </MATERIAL>
	
	<MATERIAL id="trunkTreeMaterial">                                  
        <shininess value="1" />
        <specular r="0.0" g="0.0" b="0.0" a="1" />        
        <diffuse r="0.6" g="0.298" b="0.0" a="1" />      
        <ambient r="0.1" g="0.1" b="0.1" a="1" />         
        <emission r="0.0" g="0.0" b="0.0" a="1" />       
    </MATERIAL>

    <MATERIAL id="lightPostMaterial">                                  
        <shininess value="100" />
        <specular r="0.0" g="0.0" b="0.0" a="1" />        
        <diffuse r="0.447" g="0.416" b="0.416" a="1" />       
        <ambient r="0.0" g="0.0" b="0.0" a="1" />         
        <emission r="0.0" g="0.0" b="0.0" a="1" />        
    </MATERIAL>

    <MATERIAL id="lightMaterial">                                  
        <shininess value="100" />
        <specular r="0.5" g="0.5" b="0.5" a="1" />       
        <diffuse r="0.5" g="0.5" b="0.5" a="1" />       
        <ambient r="0.0" g="0.0" b="0.0" a="1" />        
        <emission r="0.0" g="0.0" b="0.0" a="1" />        
    </MATERIAL>

    <MATERIAL id="defaultMaterial">                                  
        <shininess value="1" />
        <specular r="0.0" g="0.0" b="0.0" a="1" />        
        <diffuse r="0.4" g="0.4" b="0.4" a="1" />       
        <ambient r="0.4" g="0.4" b="0.4" a="1" />         
        <emission r="0.0" g="0.0" b="0.0" a="1" />        
    </MATERIAL>

</MATERIALS>


<NODES>

    <ROOT id="root" />     

    <NODE id="root">       
        <MATERIAL id="defaultMaterial" />         
        <TEXTURE id="null" />  

        <DESCENDANTS>
            <NODEREF id="claptraps"/>
        </DESCENDANTS>

    </NODE>

  

    <NODE id="claptraps">
    	<MATERIAL id="null" />     
        <TEXTURE id="null" />   

        <DESCENDANTS>
        	<NODEREF id="claptrap1"/>
        	<NODEREF id="claptrap2"/>
        </DESCENDANTS>

    </NODE>  

  <NODE id="claptrap1">
    	<MATERIAL id="null" />     
        <TEXTURE id="null" />   

        <DESCENDANTS>
        	<NODEREF id="claptrap"/>
        </DESCENDANTS>

    </NODE>  

      <NODE id="claptrap2">
    	<MATERIAL id="null" />     
        <TEXTURE id="null" />   

        <DESCENDANTS>
        	<NODEREF id="claptrap"/>
        </DESCENDANTS>

    </NODE>  


	  <NODE id="claptrap">
	    	<MATERIAL id="null" />     
	        <TEXTURE id="null" />   

	        <DESCENDANTS>
	        	<NODEREF id="claptrapBody"/>
	       <!-- <NODEREF id="claptrapEye"/>
	       		<NODEREF id="claptrapWheel"/>
	        	<NODEREF id="claptrapArms"/> -->
	        </DESCENDANTS>

	    </NODE>  


	  <NODE id="claptrapBody">
	    	<MATERIAL id="null" />     
	        <TEXTURE id="null" />   

	        <DESCENDANTS>
	        <!-- 	<NODEREF id="faceClap1" />
	           <NODEREF id="faceClap2" />
	          	<NODEREF id="faceClap3" />-->
	            <NODEREF id="faceClap4" /> 
	            <NODEREF id="faceClap5" />
	            <NODEREF id="faceClap6" />     
	        </DESCENDANTS>

	    </NODE>  

	  <NODE id="faceClap1">       
        <MATERIAL id="null" />     
        <TEXTURE id="null" />       		

        <TRANSLATION x="-0.36" y="0" z="0.13" />
      <ROTATION axis="x" angle="-1.5" />  
        <ROTATION axis="z" angle="-10.3" />

        <DESCENDANTS>
			<NODEREF id="trapezoid"/>        
        </DESCENDANTS>

    </NODE>

     <NODE id="faceClap2">      
        <MATERIAL id="null" />     
        <TEXTURE id="null" />       			

        <TRANSLATION x="-1.23" y="-0.01" z="-0.892" /> 
        <ROTATION axis="x" angle="-1" />  
        <ROTATION axis="z" angle="10" />
        <ROTATION axis="y" angle="180" />

        <DESCENDANTS>
			<NODEREF id="trapezoid"/>           
        </DESCENDANTS>

    </NODE>


    <NODE id="faceClap3">       
        <MATERIAL id="null" />     
        <TEXTURE id="null" />       			

        <TRANSLATION x="-0.27" y="-0.01" z="-0.88" />        
        <ROTATION axis="x" angle="-1" />  
        <ROTATION axis="x" angle="-10" />        
        <ROTATION axis="y" angle="90" />
        <SCALE sx="0.89" sy="1" sz="0.89" /> 

        <DESCENDANTS>
			<NODEREF id="trapezoid"/>           
        </DESCENDANTS>

    </NODE>


    <NODE id="faceClap4">       
        <MATERIAL id="null" />    
        <TEXTURE id="null" />       		

    

        <ROTATION axis="x" angle="010" />   

        <TRANSLATION x="1" y="0.5" z="3.5" /> 

        <ROTATION axis="y" angle="-90" />

        <DESCENDANTS>
			<NODEREF id="trapezoid"/>           
        </DESCENDANTS>
    </NODE>

     <NODE id="faceClap5">       
        <MATERIAL id="null" />    
        <TEXTURE id="null" />       		


        <TRANSLATION x="0" y="4" z="4" />   

        <ROTATION axis="x" angle="-90" />  

        <DESCENDANTS>
			<LEAF type="rectangle" args="0 3 3 0"/>   
        </DESCENDANTS>
    </NODE>


    <NODE id="faceClap6">       
        <MATERIAL id="null" />     
        <TEXTURE id="null" />       			


        <TRANSLATION x="0.5" y="0" z="0.5" />   
        <ROTATION axis="x" angle="90" />

        <DESCENDANTS>
			<LEAF  type="rectangle" args="0 2 2 0" />        
        </DESCENDANTS>

    </NODE>







	<NODE id="trapezoid"> 
		<MATERIAL id="null" />     
        <TEXTURE id="null" />         

        <DESCENDANTS>
			<NODEREF id="trapezoidRec"/>   
			<NODEREF id="trapezoidTri1"/>     
			<NODEREF id="trapezoidTri2"/>   

        </DESCENDANTS>

	</NODE>

	<NODE id="trapezoidRec"> 
		<MATERIAL id="null" />     
        <TEXTURE id="null" />         

          <ROTATION axis="y" angle="90" />

        <DESCENDANTS>
			<LEAF  type="rectangle" args="0 4 1 0" />  
        </DESCENDANTS>

	</NODE>


	<NODE id="trapezoidTri1"> 
		<MATERIAL id="null" />     
        <TEXTURE id="null" />  

        <DESCENDANTS>
			
			<LEAF  type="triangle" args="0 0 0 0 4 0 0 4 0.8 " />  

        </DESCENDANTS>

	</NODE>

	<NODE id="trapezoidTri2"> 
		<MATERIAL id="null" />     
        <TEXTURE id="null" />         

        <DESCENDANTS>
			<LEAF  type="triangle" args="0 4 -1.8 0 4 -1 0 0 -1" />  
        </DESCENDANTS>

	</NODE>



   
</NODES>



</SCENE>